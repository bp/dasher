{
  "id": "Dasher",
  "title": "Dasher",
  "version": "0.9.0-*",
  "copyright": "Copyright Drew Noakes 2015-2016",
  "authors": [ "Drew Noakes" ],
  "description": "A fast, lightweight, cross-platform serialisation tool with support for safe contract evolution.",

  "buildOptions": {
    "debugType": "portable",
    "xmlDoc": true,
    "optimize": true,
    "define": [ "UNSAFE" ],
    "allowUnsafe": true
  },

  "packOptions": {
    "owners": [ "Drew Noakes" ],
    "iconUrl": "https://raw.githubusercontent.com/drewnoakes/dasher/master/Resources/logo-128x128.png",
    "licenseUrl": "https://www.apache.org/licenses/LICENSE-2.0.html",
    "projectUrl": "https://github.com/drewnoakes/dasher",
    "requireLicenseAcceptance": false,
    "tags": [ "Serialisation", "Serialization", "MsgPack", "Communication", "Formatting", "DTO" ],
    "summary": "A fast, lightweight, cross-platform serialisation tool with support for safe contract evolution.",
    "repository": {
      "type": "git",
      "url": "https://github.com/drewnoakes/dasher.git"
    }
  },

  "dependencies": {
    "JetBrains.Annotations": {
      "version": "10.2.1",
      "type": "build"
    }
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "System.Reflection.Emit.ILGeneration": "4.0.1",
        "System.Reflection.Emit.Lightweight": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "NETStandard.Library": "1.6.0"
      }
    },

    "net45": {
      "frameworkAssemblies": {
        "System.Xml": "",
        "System.Xml.Linq": ""
      }
    }
  }
}
